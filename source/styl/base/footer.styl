.footer
	background: #131313
	padding: 50px 0
	color #fff
	&-conteiner
		width 90%
		display -webkit-flex
		display -moz-flex
		display -ms-flex
		display -o-flex
		display flex
		justify-content space-around
		margin: 0 auto
	&-img
		background #d5d5d5
		display: inline-block
		padding 10px 20px
		border-radius 20px
	&-text
		width 40%
		text-align: center