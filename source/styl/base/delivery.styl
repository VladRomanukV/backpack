.deliver
	width 90%
	margin: 0 auto
	padding: 100px 0 50px 0
	flex-wrap wrap
	&-headline
		text-align: center
		color #404040
		font-size 30px
		font-weight: bold
	&-block
		display -webkit-flex
		display -moz-flex
		display -ms-flex
		display -o-flex
		display flex
		justify-content space-between
		margin-top 70px
	&-icons
		width 30%
	&-img
		img
			display block 
			margin 0 auto
	&-text
		text-align center
		color #404040
		width 70%
		font-size 16px
		margin 20px auto 0 auto
		line-height 1.5
		font-weight: bold
		a
			color #404040